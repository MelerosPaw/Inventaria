<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="FramedPhotoViewerView">
    <attr name="displayControls" format="boolean"/>
  </declare-styleable>

  <declare-styleable name="TextImageButtonView">
    <attr name="image" format="reference"/>
    <attr name="text" format="reference|string"/>
    <attr name="minified" format="boolean"/>
    <attr name="textAlignment" format="enum">
      <enum name="start" value="0" />
      <enum name="center" value="1" />
      <enum name="end" value="2" />
    </attr>
  </declare-styleable>
</resources>